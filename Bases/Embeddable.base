views:
  - type: table
    name: Table
    filters:
      and:
        - file == this.file
  - type: cards
    name: Ancestries
    filters:
      and:
        - type == "ancestry"
  - type: cards
    name: Classes
    filters:
      and:
        - type == link("Character Creation#Choose a Class", "class")
    cardSize: 200
  - type: list
    name: Subclasses
    filters:
      and:
        - type == "subclass"
